# SMTP server settings for notifications
coin-mailserver=s01.dev.coin.surf.net


wayf.url.metadata=https://engine.dev.surfconext.nl/authentication/idp/metadata
wayf.url.sso=https://engine.dev.surfconext.nl/authentication/idp/single-sign-on
wayf.

## Properties for opensaml
ISSUING_ENTITY_ID=https\://dashboard.dev.surfconext.nl

WEB_APPLICATION_HOST_AND_PORT=dashboard.dev.surfconext.nl


# Team id that is used as definition of who is a 'dashboard_admin'.
dashboard.admin=urn\:collab\:group\:dev.surfteams.nl\:nl\:surfnet\:diensten\:dashboard_admin
# Team id that is used as definition of who is a 'dashboard.viewer'.
dashboard.viewer=urn\:collab\:group\:dev.surfteams.nl\:nl\:surfnet\:diensten\:dashboard_viewer
# Team id that is used as definition of who is a 'dashboard_super_user'.
dashboard.super.user=urn:collab:group:dev.surfteams.nl:nl:surfnet:diensten:dashboard_super_user

# Is this the dashboard instance
is.dashboard=true

api-baseurl=https\://api.dev.surfconext.nl/v1/
api-consumerkey=dev-dashboard
api-consumersecret=??
api-callbackuri=https\://dashboard.dev.surfconext.nl/apiOauthCallback?apiOauthCallback\=true

# SAB connection details
sab.endpoint=https://sab-ng.surfnet.nl/simplesaml/module.php/attributes/server.php
sab.username=??
sab.password=??

# SAB roles
admin.licentie.idp.sabRole=
admin.surfconext.idp.sabRole=SURFconextverantwoordelijke
viewer.surfconext.idp.sabRole=SURFconextbeheerder

csa.base.url=https\://csa.dev.surfconext.nl
csa.client.key=??
csa.client.secret=??
csa.oauth2.authorization.url=https://apis.dev.surfconext.nl/oauth2/token

cruncher.base.url=https://cruncher.dev.surfconext.nl/stats/v1
cruncher.client.key=??
cruncher.client.secret=??
cruncher.oauth2.authorization.url=https://apis.dev.surfconext.nl/oauth2/token

# Defines if the site is in local dev mode or deployed on the server. Needed for CSS/JS concatenation
feature.development.mode=true

# Properties file that contains metadata values for outputting SAML metadata
saml-metadata-properties-file=metadata.dashboard.properties
