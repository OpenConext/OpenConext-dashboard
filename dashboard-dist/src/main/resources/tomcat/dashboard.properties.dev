WEB_APPLICATION_HOST_AND_PORT=dashboard.dev.surfconext.nl


# Team id that is used as definition of who is a 'license contact person'.
admin.licentie.idp.teamname=urn\:collab\:group\:dev.surfteams.nl\:nl\:surfnet\:diensten\:licentiecontactpersonen
# Team id that is used as definition of who is a 'conext contact person'.
admin.surfconext.idp.teamname=urn\:collab\:group\:dev.surfteams.nl\:nl\:surfnet\:diensten\:federatiecontactpersonen
# Team id that is used as definition of who is a 'shop manager/shop admin'.
