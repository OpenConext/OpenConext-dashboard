####################################################################
##
## The following properties are used to enable/disable features
##
####################################################################
# Defines if the site is in local dev mode or deployed on the server. Used for CSS/JS concatenation
mode.dev=false

# coupling with API database for display of OAuth tokens
mode.showOauthTokens=true

# should the consent given by the user (this is coming from EB database)
mode.ebLinkActive=true

#toggle the creation of an email when a question is asked or an entity is linked / unlinked
administration.email.enabled=true

#toggle the creation of a jira ticket when a question is asked or an entity is linked / unlinked
administration.jira.ticket.enabled=true

##########################################################################
# The following properties are the responsibility of development.
##########################################################################

hibernate-selfservice.dialect=org.hibernate.dialect.MySQL5InnoDBDialect
hibernate-selfservice.hbm2ddl.auto=none
hibernate-selfservice.format_sql=true
hibernate-selfservice.show_sql=false

# Alle real implementations (and not local mocks for testing purposes)
janus.class=nl.surfnet.coin.janus.JanusRestClient
openConextApiClient=nl.surfnet.coin.api.client.OpenConextOAuthClientImpl
keyStoreClass=nl.surfnet.coin.selfservice.service.impl.ssl.KeyStore
jiraServiceClass=nl.surfnet.coin.selfservice.service.impl.JiraServiceImpl
cacheManagerClass=org.springframework.cache.ehcache.EhCacheCacheManager
sabClientClass=nl.surfnet.sab.SabClient

#Dashboard does not communicate with the Authorization Server
authorizationServerFilterClass=nl.surfnet.coin.selfservice.util.MockAuthorizationServerFilter

coin-selfservice.jdbc.driver=com.mysql.jdbc.Driver
coin-api.jdbc.driver=com.mysql.jdbc.Driver
coin-eb-db-driver=com.mysql.jdbc.Driver
coin-shindig-db-driver=com.mysql.jdbc.Driver

# global configuration of database properties, used for all data sources
coin-selfservice.database.idleConnectionTestPeriod=900
coin-selfservice.database.maxIdleTime=900

coin-selfservice.db.migrations.folder=db/migration/mysql

#which SAML attribute holds the UUID
saml-uuid-attribute=urn:oid:1.3.6.1.4.1.1076.20.40.40.1

# For deeplinks from detail pages to SURFmarket
lmngDeepLinkBaseUrl=https://www.surfmarket.nl/instelling/dlc/paginas/productomschrijving.aspx?pid=

lmngArticleCacheSeconds=21600
lmngLicenseCacheSeconds=1800

#the maximum of number mails to be send for recommendation
maxRecommendationEmails=30

