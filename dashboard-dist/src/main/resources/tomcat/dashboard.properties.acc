coin-selfservice.jdbc.password=??

coin-api.jdbc.url=jdbc:mysql://db.acc.surfconext.nl:3306/api
coin-api.jdbc.user=??
coin-api.jdbc.password=??

coin-eb-db-url=jdbc:mysql://db.acc.surfconext.nl:3306/eb
coin-eb-db-username=ebrw
coin-eb-db-password=??

# Whether to try to fetch license information from LMNG.
# Set to false to go to the surfconext version without licenses
coin-lmng-active-mode=false

# SOAP service endpoint for LMNG.
# This is a complete URL to which requests will be posted.
# Example: https://lmng01.dev.coin.surf.net:8444/CrmService.svc
coin-lmng-endpoint=https\://na.example.org

# If set to true, requests/responses will be written to a file in the java.io.tmpdir directory.
coin-lmng-debug=false

coin-shindig-db-url=jdbc:mysql://db.acc.surfconext.nl:3306/shindig
coin-shindig-db-username=shindigrw
coin-shindig-db-password=??

# SMTP server settings for notifications
coin-mailserver=localhost
coin-mailserver-port=25

# Email address that is the sender of invitations/requests
systemEmail=SURFconext <no-reply@surfconext.nl>

# Email address that recieves error notifications
coin-errormail-to=coin-logs@list.surfnet.nl
coin-administrative-email=coin-logs@list.surfnet.nl

