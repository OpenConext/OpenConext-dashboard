# Email address that recieves error notifications
coin-errormail-to=coin-logs@list.surfnet.nl
coin-administrative-email=coin-logs@list.surfnet.nl

wayf.url.metadata=https://engine.acc.surfconext.nl/authentication/idp/metadata
wayf.url.sso=https://engine.acc.surfconext.nl/authentication/idp/single-sign-on
wayf.certificate=??

## Properties for opensaml
ISSUING_ENTITY_ID=https\://dashboard.acc.surfconext.nl

WEB_APPLICATION_HOST_AND_PORT=dashboard.acc.surfconext.nl

# Team id that is used as definition of who is a 'dashboard_admin'.
dashboard.admin=urn\:collab\:group\:acc.surfteams.nl\:nl\:surfnet\:diensten\:dashboard_admin
# Team id that is used as definition of who is a 'dashboard.viewer'.
dashboard.viewer=urn\:collab\:group\:acc.surfteams.nl\:nl\:surfnet\:diensten\:dashboard_viewer
# Team id that is used as definition of who is a 'dashboard_super_user'.
dashboard.super.user=urn:collab:group:acc.surfteams.nl:nl:surfnet:diensten:dashboard_super_user

# Is this the dashboard instance
is.dashboard=true

api-baseurl=https\://api.acc.surfconext.nl/v1/
api-consumerkey=https\://dashboard.acc.surfconext.nl
api-consumersecret=??
api-callbackuri=https\://dashboard.acc.surfconext.nl/apiOauthCallback?apiOauthCallback\=true

# SAB connection details
sab.endpoint=https://sab.surfnet.nl/simplesaml/module.php/attributes/server.php
sab.username=??
sab.password=??

# SAB roles
admin.licentie.idp.sabRole=
admin.surfconext.idp.sabRole=SURFconextverantwoordelijke
viewer.surfconext.idp.sabRole=SURFconextbeheerder

csa.base.url=https\://csa.acc.surfconext.nl
csa.client.key=??
csa.client.secret=??
csa.oauth2.authorization.url=https://apis.acc.surfconext.nl/oauth2/token

cruncher.base.url=https://cruncher.acc.surfconext.nl/stats/v1
cruncher.client.key=??
cruncher.client.secret=??
cruncher.oauth2.authorization.url=https://apis.acc.surfconext.nl/oauth2/token


# Properties file that contains metadata values for outputting SAML metadata
saml-metadata-properties-file=metadata.dashboard.properties