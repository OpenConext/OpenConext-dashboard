# SMTP server settings for notifications
coin-mailserver=s01.dev.coin.surf.net


wayf.url.metadata=https://engine.test.surfconext.nl/authentication/idp/metadata
wayf.url.sso=https://engine.test.surfconext.nl/authentication/idp/single-sign-on
wayf.

## Properties for opensaml
ISSUING_ENTITY_ID=https\://dashboard.test.surfconext.nl

WEB_APPLICATION_HOST_AND_PORT=dashboard.test.surfconext.nl


# Team id that is used as definition of who is a 'showroom_admin'.
showroom.admin=urn\:collab\:group\:test.surfteams.nl\:nl\:surfnet\:diensten\:licentiecontactpersonen
# Team id that is used as definition of who is a 'dashboard_admin'.
dashboard.admin=urn\:collab\:group\:test.surfteams.nl\:nl\:surfnet\:diensten\:federatiecontactpersonen
# Team id that is used as definition of who is a 'dashboard.viewer'.
dashboard.viewer=urn\:collab\:group\:test.surfteams.nl\:nl\:surfnet\:diensten\:federatiecontactpersonen
# Is this the dashboard instance
is.dashboard=true

api-baseurl=https\://api.test.surfconext.nl/v1/
api-consumerkey=https\://dashboard.test.surfconext.nl
api-consumersecret=??
api-callbackuri=https\://dashboard.test.surfconext.nl/apiOauthCallback?apiOauthCallback\=true

# SAB connection details
sab.endpoint=https://sab-ng.surfnet.nl/simplesaml/module.php/attributes/server.php
sab.username=??
sab.password=??

# SAB roles
admin.licentie.idp.sabRole=
admin.surfconext.idp.sabRole=SURFconextverantwoordelijke
viewer.surfconext.idp.sabRole=SURFconextbeheerder

csa.base.url=https://csa.test.surfconext.nl/csa/
csa.client.key=??
csa.client.secret=??
csa.oauth2.authorization.url=https://apis.test.surfconext.nl/oauth2/token

