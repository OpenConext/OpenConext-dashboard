##########################################################################
# The following properties are the responsibility of Maintenance. The    #
# values depend on the environment and are unknown to the developers     #
##########################################################################


# SMTP server settings for notifications
coin-mailserver=s01.dev.coin.surf.net


wayf.url.metadata=https://engine.acc.showroom.surfconext.nl/authentication/idp/metadata
wayf.url.sso=https://engine.acc.showroom.surfconext.nl/authentication/idp/single-sign-on
wayf.

## Properties for opensaml
ISSUING_ENTITY_ID=https\://dashboard.test.showroom.surfconext.nl

WEB_APPLICATION_HOST_AND_PORT=dashboard.test.showroom.surfconext.nl

# Team id that is used as definition of who is a 'dashboard_admin'.
dashboard.admin=urn\:collab\:group\:test.surfteams.nl\:nl\:surfnet\:diensten\:federatiecontactpersonen
# Team id that is used as definition of who is a 'dashboard.viewer'.
dashboard.viewer=urn\:collab\:group\:test.surfteams.nl\:nl\:surfnet\:diensten\:federatiecontactpersonen
# Team id that is used as definition of who is a 'dashboard_super_user'.
dashboard.super.user=urn:collab:group:dev.surfteams.nl:nl:surfnet:diensten:dashboard_super_user

is.dashboard=true

api-baseurl=https\://api.acc.showroom.surfconext.nl/v1/
api-consumerkey=https://showroom.test.showroom.surfconext.nl
api-consumersecret=Urban Cookie Collective
api-callbackuri=https\://showroom.test.showroom.surfconext.nl/apiOauthCallback?apiOauthCallback=true

# SAB connection details
sab.endpoint=https://sab-ng.surfnet.nl/simplesaml/module.php/attributes/server.php
sab.username=??
sab.password=??

# SAB roles
admin.licentie.idp.sabRole=
admin.surfconext.idp.sabRole=SURFconextverantwoordelijke
viewer.surfconext.idp.sabRole=SURFconextbeheerder

csa.base.url=https://csa.test.showroom.surfconext.nl
csa.client.key=dashboardtestshowroomsurfconextnl
csa.client.secret=??
csa.oauth2.authorization.url=https://apis.test.showroom.surfconext.nl/oauth2/token

cruncher.base.url=https://cruncher.acc.showroom.surfconext.nl/stats/v1
cruncher.client.key=dashboardtestshowroomsurfconextnl-to-cruncheracc
cruncher.client.secret=
cruncher.oauth2.authorization.url=https://apis.test.showroom.surfconext.nl/oauth2/token

####################################################################
##
## The following properties are used to enable/disable features
##
####################################################################
# Defines if the site is in local dev mode or deployed on the server. Needed for CSS/JS concatenation
feature.development.mode=true


# Properties file that contains metadata values for outputting SAML metadata
saml-metadata-properties-file=metadata.dashboard.properties
